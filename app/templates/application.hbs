<h1 class="title">Know Your ember-cli</h1>
<p><Input class="commandbar" type="text" @value={{this.command.cmd}}/></p>
<p>
<label> Use Alias</label>
<Input type="checkbox" {{on 'click' this.command.toggleAlias}}/></p>

<nav class="header-nav">
  <ul>
    <li>
      <LinkTo @route="index">Home</LinkTo>
    </li>
  </ul>
</nav>

