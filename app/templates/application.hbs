      <LinkTo @route="asset-sizes">asset-sizes</LinkTo>
    </li>
    <li>
      <LinkTo @route="build">build</LinkTo>
    </li>
    <li>
      <LinkTo @route="destroy">destroy</LinkTo>
    </li>
    <li>
      <LinkTo @route="generate">generate</LinkTo>
    </li>
    <li>
      <LinkTo @route="help">help</LinkTo>
    </li>
    <li>
      <LinkTo @route="init">init</LinkTo>
    </li>
    <li>
      <LinkTo @route="install">install</LinkTo>
    </li>
    <li>
      <LinkTo @route="new">new</LinkTo>
    </li>
    <li>
      <LinkTo @route="serve">serve</LinkTo>
    </li>
    <li>
      <LinkTo @route="test">test</LinkTo>
    </li>
    <li>
      <LinkTo @route="uninstall-npm">uninstall-npm</LinkTo>
